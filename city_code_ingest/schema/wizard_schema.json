{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "jurisdiction": {
      "type": "object",
      "properties": {
        "city": {"type": "string"},
        "state": {"type": "string"},
        "version": {"type": "string"},
        "last_updated": {"type": "string"},
        "source_url": {"type": "string"}
      },
      "required": ["city", "state", "version", "last_updated"]
    },
    "titles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title_number": {"type": "integer"},
          "title_name": {"type": "string"},
          "chapters": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "chapter_number": {"type": "integer"},
                "chapter_name": {"type": "string"},
                "sections": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "section_id": {"type": "string"},
                      "section_title": {"type": "string"},
                      "breadcrumbs": {
                        "type": "array",
                        "items": {"type": "string"}
                      },
                      "topics": {
                        "type": "array",
                        "items": {"type": "string"}
                      },
                      "decision_points": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "rad_id": {"type": "string"},
                            "rad_text": {"type": "string"},
                            "question": {"type": "string"},
                            "po_links": {
                              "type": "array",
                              "items": {"type": "string"}
                            },
                            "po_details": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "po_id": {"type": "string"},
                                  "text": {"type": "string"},
                                  "page": {"type": "integer"},
                                  "span": {
                                    "type": "array",
                                    "items": {"type": "number"},
                                    "minItems": 2,
                                    "maxItems": 2
                                  }
                                },
                                "required": ["po_id", "text"]
                              }
                            },
                            "ead_links": {
                              "type": "array",
                              "items": {"type": "string"}
                            },
                            "ead_details": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "ead_id": {"type": "string"},
                                  "text": {"type": "string"},
                                  "page": {"type": "integer"},
                                  "span": {
                                    "type": "array",
                                    "items": {"type": "number"},
                                    "minItems": 2,
                                    "maxItems": 2
                                  }
                                },
                                "required": ["ead_id", "text"]
                              }
                            },
                            "source_refs": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "page": {"type": "integer"},
                                  "span": {
                                    "type": "array",
                                    "items": {"type": "number"},
                                    "minItems": 2,
                                    "maxItems": 2
                                  }
                                },
                                "required": ["page", "span"]
                              }
                            },
                            "no_po_applicable": {"type": "boolean"}
                          },
                          "required": ["rad_id", "question", "source_refs"]
                        }
                      }
                    },
                    "required": ["section_id", "section_title", "decision_points", "breadcrumbs", "topics"]
                  }
                }
              },
              "required": ["chapter_number", "sections"]
            }
          }
        },
        "required": ["title_number", "chapters"]
      }
    }
  },
  "required": ["jurisdiction", "titles"]
}
