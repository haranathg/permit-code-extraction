{
  "type": "object",
  "properties": {
    "jurisdiction": {
      "type": "object",
      "properties": {
        "city": {"type": "string"},
        "state": {"type": "string"},
        "version": {"type": "string"},
        "last_updated": {"type": "string"},
        "source_url": {"type": "string"}
      },
      "required": ["city", "state", "version"]
    },
    "titles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title_number": {"type": "integer"},
          "title_name": {"type": "string"},
          "chapters": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "chapter_number": {"type": "integer"},
                "chapter_name": {"type": "string"},
                "sections": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "section_id": {"type": "string"},
                      "section_title": {"type": "string"},
                      "text": {"type": "string"},
                      "topics": {"type": "array", "items": {"type": "string"}},
                      "references": {"type": "array", "items": {"type": "string"}},
                      "requires_documents": {"type": "array", "items": {"type": "string"}},
                      "effective_date": {"type": "string"},
                      "breadcrumb": {"type": "string"},
                      "decision_points": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "question_id": {"type": "string"},
                            "text": {"type": "string"},
                            "response_options": {"type": "array", "items": {"type": "string"}},
                            "po_references": {"type": "array", "items": {"type": "string"}},
                            "guidance": {"type": "string"}
                          },
                          "required": ["question_id", "text", "response_options"]
                        }
                      }
                    },
                    "required": ["section_id", "text"]
                  }
                }
              },
              "required": ["chapter_number", "sections"]
            }
          }
        },
        "required": ["title_number", "chapters"]
      }
    }
  },
  "required": ["jurisdiction", "titles"]
}
